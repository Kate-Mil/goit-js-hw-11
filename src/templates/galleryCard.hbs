{{#each this}}
<div class="photo-card">
  <a href="{{largeImageURL}}" class="gallery-item">
  <img src="{{webformatURL}}" alt="{{tags}}" width="340px" height="220px" loading="lazy" class="gallery-image" />
  </a>
  <div class="info">
    <p class="info-item">
    <b>Likes </b><br>{{likes}}
    </p>
    <p class="info-item">
      <b>Views </b><br>{{views}}
    </p>
    <p class="info-item">
      <b>Comments </b><br>{{comments}}
    </p>
    <p class="info-item">
      <b>Downloads </b><br>{{downloads}}
    </p>
  </div>
</div>
{{/each}}